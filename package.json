{
  "name": "unplugin-vue-gen-routes",
  "type": "module",
  "version": "0.0.7",
  "packageManager": "pnpm@10.3.0",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/pot-code/unplugin-vue-gen-routes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pot-code/unplugin-vue-gen-routes.git"
  },
  "bugs": {
    "url": "https://github.com/pot-code/unplugin-vue-gen-routes/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "vue-router",
    "pages",
    "filesystem",
    "types",
    "typed",
    "vue",
    "router"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./rspack": {
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./types": {
      "import": "./dist/types.d.ts",
      "require": "./dist/types.d.cts"
    },
    "./runtime": {
      "import": "./dist/runtime.js",
      "require": "./dist/runtime.cjs"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "client.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@rspack/core": "^1",
    "rollup": "^3",
    "vue-router": "^4",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@rspack/core": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@vue-macros/common": "3.0.0-beta.8",
    "@vue/compiler-sfc": "^3.5.13",
    "ast-walker-scope": "^0.7.1",
    "chokidar": "^4.0.3",
    "fast-glob": "^3.3.3",
    "json5": "^2.2.3",
    "local-pkg": "^1.1.1",
    "mlly": "^1.7.4",
    "pathe": "^2.0.3",
    "picomatch": "^4.0.2",
    "scule": "^1.3.0",
    "unplugin": "^2.3.2",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@types/node": "^22.13.1",
    "@types/picomatch": "^4.0.0",
    "bumpp": "^10.1.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "rollup": "^4.34.6",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unplugin-vue-gen-routes": "workspace:*",
    "vitest": "^3.0.5",
    "webpack": "^5.97.1"
  }
}
